<h1>投稿一覧</h1>
<table>
  <tr>
    <th>title</th>
    <th>blog</th>
    <th>detail</th>
    <th>edit</th>
    <th>delete</th>
  </tr>
  <% @blogs.each do |blog|%>
    <tr>
      <td><%= blog.title %></td>
      <td><%= blog.content %></td>
      <td><%= link_to "detail",blog_path(blog.id) %></td>
      <td><%= link_to "edit",edit_blog_path(blog.id) %></td>
      <td><%= link_to "delete",blog_path(blog.id),method: :delete %></td>
    </tr>
  <% end %>
  <%= link_to "新規投稿", new_blog_path %>
</table>